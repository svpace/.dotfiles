set autoindent
set morespace
unset mouse
set tabsize 4
set smarthome

bind ^C copy main
#bind F1 help all
bind ^F whereis main
bind ^H replace main
bind ^I casesens replace
bind ^I casesens search
bind ^Q cancel all
bind ^Q exit main
bind ^Q exit help
bind ^S writeout main
bind ^V paste main
bind ^X cut main
bind ^Y redo main
bind ^Z undo main
